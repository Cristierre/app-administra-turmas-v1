<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@android:color/holo_red_dark"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Turmas">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="568dp"
        android:layout_height="811dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.444"
        app:srcCompat="@drawable/books"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-33dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="244dp"
        android:layout_height="195dp"
        app:srcCompat="@drawable/logo_fadergs"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="-20dp" />

    <TextView
        android:id="@+id/tvTurma"
        android:layout_width="454dp"
        android:layout_height="43dp"
        android:background="#ED0707"
        android:text="TURMAS"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="175dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvDisciplinas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#4CAF50"
        android:text="DISCIPLINAS"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/tvTurno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#4CAF50"
        android:text="TURNO"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="201dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/tvValidacao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#4CAF50"
        android:text="VALIDAÇÃO"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="218dp" />

    <EditText
        android:id="@+id/etDisciplina1"
        android:layout_width="136dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="DEV MOBILE"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="253dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/etTurno3"
        android:layout_width="76dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="NOITE"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="252dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/etTurno2"
        android:layout_width="75dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="TARDE"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="291dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/etTurno1"
        android:layout_width="76dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="MANHÃ"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="338dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/etDisciplina2"
        android:layout_width="136dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="JAVA"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="294dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/etDisciplina3"
        android:layout_width="137dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="C++"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="337dp"
        tools:ignore="MissingConstraints" />

    <Switch
        android:id="@+id/sSwitch1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:showText="true"
        android:textOff="OFF"
        android:textOn="ON"
        android:trackTint="#3F51B5"
        tools:checked="false"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="348dp"
        tools:layout_editor_absoluteY="255dp" />

    <Switch
        android:id="@+id/sSwitch2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:showText="true"
        android:trackTint="#3F51B5"
        tools:checked="false"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="346dp"
        tools:layout_editor_absoluteY="294dp" />

    <Switch
        android:id="@+id/sSwitch3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:showText="true"
        android:trackTint="#3F51B5"
        tools:checked="false"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="348dp"
        tools:layout_editor_absoluteY="338dp" />

    <Button
        android:id="@+id/btnSalvar"
        style="@style/btnForms"
        android:text="SALVAR"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="484dp" />


</androidx.constraintlayout.widget.ConstraintLayout>