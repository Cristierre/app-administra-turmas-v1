<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="imgLogo" parent="AppTheme">
        <item name="android:layout_width">400dp</item>
        <item name="android:layout_height">350dp</item>
    </style>

    <style name="formulario" parent="AppTheme">
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/books</item>
    </style>

    <style name="etForm" parent="AppTheme">
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_width">250dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">60px</item>
        <item name="android:background">#DFFFFFFF</item>
    </style>
    <style name="btnForms" parent="AppTheme">
        <item name="android:layout_width">167dp</item>
        <item name="android:layout_height">61dp</item>
        <item name="android:backgroundTint">#D51212</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">23sp</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
